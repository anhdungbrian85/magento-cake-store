<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="sales_order" resource="default" engine="innodb">
        <column xsi:type="int" name="store_location_id" nullable="true" comment="Store Location ID"/>
    </table>
    <table name="quote" resource="default" engine="innodb">
        <column xsi:type="int" name="store_location_id" nullable="true" comment="Store Location ID"/>
    </table>
    <table name="amasty_amlocator_schedule" resource="default" engine="innodb" comment="amasty_amlocator_schedule">
        <column xsi:type="varchar" name="interval" nullable="false" default="60" length="255" comment="Schedule Interval"/>
    </table>
</schema>
